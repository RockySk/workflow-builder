{
  "name": "@i-vresse/wb-core",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "ts-standard"
  },
  "devDependencies": {
    "postcss": "^8.4.14",
    "ts-standard": "^11.0.0",
    "tsup": "^6.1.3"
  },
  "tsup": {
    "entry": [
      "src/index.tsx"
    ],
    "dts": true,
    "target": "node16",
    "format": "esm"
  }
}
